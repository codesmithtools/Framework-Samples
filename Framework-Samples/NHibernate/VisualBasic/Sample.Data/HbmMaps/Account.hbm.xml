<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Sample.Data.Generated.BusinessObjects" assembly="Sample.Data">
	<class name="Sample.Data.Generated.BusinessObjects.Account, Sample.Data" table="[dbo].[Account]" lazy="true">
		<id name="Id" column="[AccountID]">
		 	<generator class="native" />
		</id>
		
		<property name="Email" column="[Email]" />
		<property name="FirstName" column="[FirstName]" />
		<property name="LastName" column="[LastName]" />
		<property name="Address1" column="[Address1]" />
		<property name="Address2" column="[Address2]" />
		<property name="City" column="[City]" />
		<property name="State" column="[State]" />
		<property name="Zip" column="[Zip]" />
		<property name="Country" column="[Country]" />
		<property name="Phone" column="[Phone]" />
		<property name="Number" column="[Number]" />
		<many-to-one name="Profile" column="[UniqueID]" class="Profile" />
	</class>
</hibernate-mapping>