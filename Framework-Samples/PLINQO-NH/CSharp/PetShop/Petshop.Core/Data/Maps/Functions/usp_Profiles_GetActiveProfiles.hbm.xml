<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Petshop.Data.Entities" assembly="Petshop.Core">
  <sql-query name="UspProfilesGetActiveProfile" >
    <return class="Profile" >
      <return-property name="UniqueID" column="UniqueID" />
      <return-property name="Username" column="Username" />
      <return-property name="ApplicationName" column="ApplicationName" />
      <return-property name="IsAnonymous" column="IsAnonymous" />
      <return-property name="LastActivityDate" column="LastActivityDate" />
      <return-property name="LastUpdatedDate" column="LastUpdatedDate" />
    </return>
    <query-param name="LastActiveDateStart" type="DateTime" />
    <query-param name="LastActiveDateEnd" type="DateTime" />
    exec [usp_Profiles_GetActiveProfiles] :lastActiveDateStart, lastActiveDateEnd
  </sql-query>
</hibernate-mapping>

